{
	"manifest_version": 2,
	"name": "Big IP Blocker",
	"version": "0.1",
	"background": {
	  "scripts": [
		"background.js",
		"reload.js"
	]
	},
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["lib/jquery.min.js", "content.js"]
	  }
	],
	"browser_action": {
	  "default_icon": "icon.png",
	  "default_popup": "main.html"

	},
	"permissions": [
		"<all_urls>",
		"tabs",
		"webRequest",
		"webRequestBlocking"
	]
  }