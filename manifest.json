{
	"manifest_version": 2,
	"name": "Big IP Blocker",
	"version": "0.1",
	"background": {
	  "scripts": ["background.js"]
	},
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["jquery.min.js", "content.js"]
	  }
	],
	"browser_action": {
	  "default_icon": "icon.png"
	},
	"permissions": [
		"<all_urls>",
		"tabs",
		"webRequest",
		"webRequestBlocking"
	]
  }